{
  "name": "@whoj/box-nuxt",
  "type": "module",
  "version": "1.0.0-beta.3",
  "packageManager": "pnpm@8.3.0",
  "description": "Nuxt Module for Box Ui Elements as Vue (2 & 3) Component",
  "keywords": [
    "box",
    "vue",
    "vue 3",
    "nuxt",
    "nuxt 3",
    "box ui elements",
    "vue box ui elements"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "develop": "./src/module.ts",
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    ">=5.0": {
      "*": [
        "./dist/types.d.ts",
        "./dist/module.d.ts"
      ]
    },
    "<=4.9": {
      "*": [
        "./dist/types.d.ts",
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.3",
    "@whoj/box-vue": "workspace:^",
    "@whoj/utils": "^1.8.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.3",
    "@nuxt/test-utils": "^3.7.3",
    "@vue-macros/nuxt": "^1.8.1",
    "@whoj/box-icons": "workspace:^",
    "nuxt": "^3.7.3"
  },
  "peerDependencies": {
    "@whoj/box-icons": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@whoj/box-icons": {
      "optional": true
    }
  },
  "repository": {
    "type": "github",
    "directory": "packages/nuxt",
    "url": "https://github.com/who-jonson/box-vue"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
