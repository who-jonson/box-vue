{
  "name": "@whoj/box-vue-monorepo",
  "packageManager": "pnpm@8.6.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vue": "pnpm -C packages/vue run dev",
    "dev:nuxt": "pnpm -C packages/nuxt run dev",
    "build": "pnpm -r run prepublishOnly",
    "play": "run-p dev:vue play:vite",
    "play:vite": "vite examples/vue-vite --host",
    "release": "bumpp packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "lint": "eslint \"{packages/**/*,*}.{js,ts,tsx,vue}\"",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@box/cldr-data": "^41.0.1",
    "@hapi/address": "^5.1.1",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.7.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^18.17.18",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.4.1",
    "@whoj/box-icons": "workspace:^",
    "@whoj/box-nuxt": "workspace:^",
    "@whoj/box-vue": "workspace:^",
    "@whoj/boxdk": "workspace:^",
    "@whoj/eslint-config": "^1.5.0",
    "@whoj/utils": "^1.8.0",
    "axios": "^0.27.2",
    "box-ui-elements": "^18.1.0",
    "bumpp": "^9.2.0",
    "changelogen": "^0.5.5",
    "core-js": "^3.32.2",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "cross-env": "^7.0.3",
    "draft-js": "^0.11.7",
    "esbuild": "^0.19.3",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.49.0",
    "filesize": "^10.0.12",
    "form-serialize": "^0.7.2",
    "formik": "^2.4.5",
    "ilib-tree-node": "^1.3.0",
    "immutable": "5.0.0-beta.4",
    "intl-messageformat": "^10.5.3",
    "js-sha1": "0.6.0",
    "jsuri": "^1.3.1",
    "lodash": "^4.17.21",
    "magicast": "^0.3.0",
    "message-accumulator": "^2.2.1",
    "mousetrap": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "pikaday": "^1.8.2",
    "query-string": "8.1.0",
    "regenerator-runtime": "^0.14.0",
    "rollup": "^3.29.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "sanitize-html": "2.11.0",
    "sass": "1.68.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "tabbable": "^6.2.0",
    "terser": "^5.20.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unplugin-vue": "^4.3.5",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-jsx": "^0.2.2",
    "unplugin-vue-macros": "^2.5.1",
    "uuid": "^9.0.1",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.4",
    "vitest": "^0.34.5",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue-tsc": "^1.8.13"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "popper.js": "*",
      "@hapi/address": "*",
      "@formatjs/intl-utils": "*",
      "intl-messageformat-parser": "*",
      "@formatjs/intl-unified-numberformat": "*"
    },
    "overrides": {
      "core-js": "$core-js",
      "vue-demi": "$vue-demi",
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4"
    },
    "peerDependencyRules": {
      "allowAny": [
        "sass",
        "react",
        "react-dom",
        "sanitize-html",
        "@hapi/address"
      ],
      "ignoreMissing": [
        "@box/*",
        "@vue/*",
        "@vueuse/*",
        "@vue-macros/*",
        "react-*",
        "react",
        "nuxi"
      ]
    }
  },
  "repository": "who-jonson/box-vue"
}
