{
  "name": "@whoj/box-vue-monorepo",
  "packageManager": "pnpm@8.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:vue": "pnpm -C packages/vue run dev",
    "dev:nuxt": "pnpm -C packages/nuxt run dev",
    "play": "run-p dev:vue play:vite",
    "play:vite": "vite examples/vue-vite --host",
    "release": "pnpm lint:fix && bumpp packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "lint": "eslint \"{packages/**/*,*}.{js,ts,tsx,vue}\"",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@box/cldr-data": "^34.2.0",
    "@hapi/address": ">=2.0.0 <2.1.2",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/test-utils": "^3.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.6",
    "@vue/tsconfig": "^0.3.2",
    "@vueuse/core": "^10.1.2",
    "@whoj/box-icons": "workspace:^",
    "@whoj/box-nuxt": "workspace:^",
    "@whoj/box-vue": "workspace:^",
    "@whoj/boxdk": "^1.0.0-beta.7",
    "@whoj/eslint-config": "^1.4.0",
    "@whoj/utils": "^1.7.4",
    "axios": "^0.25.0",
    "box-ui-elements": "^17.0.0",
    "bumpp": "^9.1.0",
    "changelogen": "^0.5.3",
    "classnames": "^2.2.5",
    "color": "^3.1.2",
    "cross-env": "^7.0.3",
    "draft-js": "^0.10.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.39.0",
    "filesize": "^4.1.2",
    "form-serialize": "^0.7.2",
    "formik": "^2.0.0",
    "ilib-tree-node": "^1.2.0",
    "immutable": "^3.7.4",
    "intl-messageformat": "^7.8.4",
    "js-sha1": "0.6.0",
    "jsuri": "^1.3.1",
    "lodash": "^4.17.5",
    "magicast": "^0.2.4",
    "message-accumulator": "^2.1.1",
    "mousetrap": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "pikaday": "^1.8.0",
    "query-string": "5.1.1",
    "regenerator-runtime": "^0.13.2",
    "rollup": "^3.21.5",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "sanitize-html": "1.21.1",
    "sass": "1.34.1",
    "scroll-into-view-if-needed": "^2.2.20",
    "tabbable": "^1.1.2",
    "terser": "^5.17.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "unplugin-vue": "^4.1.0",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-jsx": "^0.2.0",
    "unplugin-vue-macros": "^2.1.4",
    "uuid": "^8.3.2",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.6.4"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "popper.js": "*",
      "@hapi/address": "*",
      "@formatjs/intl-utils": "*",
      "intl-messageformat-parser": "*",
      "@formatjs/intl-unified-numberformat": "*"
    },
    "overrides": {
      "core-js": "^3.30.0",
      "vue-demi": "^0.14.1",
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4"
    },
    "peerDependencyRules": {
      "allowAny": [
        "sass",
        "react",
        "react-dom",
        "sanitize-html",
        "@hapi/address"
      ],
      "ignoreMissing": [
        "@vue/*",
        "@vueuse/*",
        "react-*",
        "react"
      ]
    }
  },
  "repository": "who-jonson/box-vue"
}
